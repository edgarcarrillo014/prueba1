<widget xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" id="com.CualquierNombre" version="1.0.0">
<name>Dreameater</name>
 <!-- nombre de la aplicacion -->
<description> Describcion </description>
<author email="" href="http://cordova.io"> tu nombre y correo </author>
<content src="index.html"/>
<plugin name="cordova-plugin-whitelist" spec="1"/>
<access origin="*"/>
<allow-intent href="http://*/*"/>
<allow-intent href="https://*/*"/>
<allow-intent href="tel:*"/>
<allow-intent href="sms:*"/>
